{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {
    "homepage": "${mod_display_url}",
    "issues": "${mod_issues_url}",
    "sources": "${mod_sources_url}"
  },
  "license": "LGPLv3",
  "icon": "assets/effortless/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.huskuraft.effortless.fabric.platform.FabricInitializer"
    ],
    "client": [
      "dev.huskuraft.effortless.fabric.platform.FabricClientInitializer"
    ]
  },
  "mixins": [
    "effortless.fabric.mixins.json"
  ],
  "accessWidener": "effortless.accesswidener",
  "depends": {
    "minecraft": "${minecraft_version_range}"
  }
}
